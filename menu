if [[ $ssh_ws == "running" ]]; then
    status_ws_epro="${green}ONâœ“${NC}"
else
    status_ws_epro="${RED}ğŸ”´${NC} "
fi

# STATUS SERVICE HAPROXY
if [[ $haproxy_service == "running" ]]; then 
   status_haproxy="${green}ONâœ“${NC}"
else
   status_haproxy="${RED}ğŸ”´${NC} "
fi

