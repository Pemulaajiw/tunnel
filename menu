    d1=$(date -d "$1" +%s)
    d2=$(date -d "$2" +%s)
    echo -e "${BIYellow}         $NC$COLBG1  Expiry In$NC     $Blue:$NC$BIBlue Registered "
}
mai="datediff "$Exp" "$DATE""
echo -e " ${COLOR1}â•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®â•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®â•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e " ${COLOR1}â”‚ ${NC}${COLBG1} SSH$NC : $status_ssh" "        ${COLBG1} NGINX$NC : $status_nginx" "        ${COLBG1} XRAY$NC : $status_xray      $NC${COLOR1}â”‚$NC" 
echo -e " ${COLOR1}â”‚ ${NC}${COLBG1} WS-ePRO$NC : $status_ws_epro" "    ${COLBG1} DROPBEAR$NC : $status_dropbear" "     ${COLBG1} HAPROXY$NC : $status_haproxy   $NC${COLOR1}â”‚$NC" 
echo -e " ${COLOR1}â•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯â•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯â•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${NC}"
echo -e "${COLOR1} â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”${NC}"
echo -e "   $COLOR1 ${COLBG1}Traffic${NC}      ${COLBG1}Today       Yesterday       Month   ${NC}"
echo -e "   $COLOR1 ${COLBG1}Download${NC}   ${COLBG1}$today_tx $today_txv      $yesterday_tx $yesterday_txv      $month_tx $month_txv   ${NC}"
